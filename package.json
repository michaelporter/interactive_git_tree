{
  "name": "interactive-git-network",
  "version": "0.0.2",
  "description": "Interactive git-like tree ",
  "main": "index.js",
  "scripts": {
    "build:css": "sass --update src/sass:assets/css/",
    "watch:css": "watch 'npm run build:css' src/sass/",
    "build:js": "babel src/js --out-dir js --source-maps",
    "watch:js": "watch 'npm run build:js' src/js",
    "build": "npm run build:js && npm run build:css",
    "env": "env",
    "test": "echo 'hello!'",
    "build:watch": "parallelshell 'npm run watch:js' 'npm run watch:css'"
  },
  "keywords": [
    "git"
  ],
  "babel": {},
  "author": "Michael Porter <michaelporter.0988@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "parallelshell": "^2.0.0",
    "watch": "^0.16.0"
  }
}
